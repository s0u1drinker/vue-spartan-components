{
  "name": "vue-spartan-components",
  "license": "MIT",
  "version": "0.0.9",
  "description": "Vue 3 component library.",
  "keywords": [
    "vue",
    "components",
    "ui",
    "library"
  ],
  "type": "module",
  "main": "./dist/vue-spartan-components.umd.js",
  "module": "./dist/vue-spartan-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-spartan-components.es.js",
      "require": "./dist/vue-spartan-components.umd.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.ts,.js,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.js,.tsx --fix",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-vitest": "^9.1.13",
    "@storybook/vue3-vite": "^9.1.13",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.13",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "storybook": "^9.1.13",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  }
}
