{
  "name": "vue-spartan-components",
  "license": "MIT",
  "version": "0.0.3",
  "description": "Vue 3 component library.",
  "keywords": [
    "vue",
    "components",
    "ui",
    "library"
  ],
  "type": "module",
  "main": "./dist/vue-spartan-components.umd.js",
  "module": "./dist/vue-spartan-components.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-spartan-components.es.js",
      "require": "./dist/vue-spartan-components.umd.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.ts,.js,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.js,.tsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  }
}
